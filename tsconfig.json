{
  "compilerOptions": {
    "noEmit": true,
    "moduleResolution": "node",
    "module": "esnext",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "target": "esnext",
    "allowJs": true,
    "checkJs": true,
    "strict": true,
    "skipLibCheck": true,
    "useUnknownInCatchVariables": false,
  },
  "include": [
    "src"
  ]
}
